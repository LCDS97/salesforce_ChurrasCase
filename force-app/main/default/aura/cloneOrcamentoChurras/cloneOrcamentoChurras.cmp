<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction"
access="global" controller="ClonarOrcamentoChurrasController">
<!-- Neutral variant (default) -->
    <aura:attribute name="recordId" type="String" />
    <aura:handler name="init" value="{! this }" action="{! c.init }"/>

    <lightning:navigation aura:id="navService"/>
    <div class="slds-p-around--xx-large slds-text-align--center">
        <div class="slds-spinner slds-spinner_medium">
            <div class="slds-spinner__dot-a"></div>
            <div class="slds-spinner__dot-b"></div>
        </div>
        <div class="slds-m-bottom_large slds-text-heading_medium">
            Gerando Clone...
        </div>
	</div>

<!-- <lightning:button label="Clonar OrÃ§amento Churras" title="Neutral action" onclick="{! c.clonarAvaliacao }" /> -->
</aura:component>
